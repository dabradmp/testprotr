{
  "name": "protractor-typescript-framework",
  "version": "1.0.0",
  "description": "protractor + typescript test framework example",
  "author": "andriyze",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andriyze/proTR.git"
  },
  "scripts": {
    "postinstall": "node node_modules/protractor/bin/webdriver-manager update",
    "pretest": "npm run tsc",
    "test": "npm run protractor",
    "lint": "node node_modules/.bin/tslint 'test/**/*.ts'",
    "protractor": "node node_modules/protractor/bin/protractor",
    "start": "node node_modules/protractor/bin/webdriver-manager start --logging=log.conf",
    "tsc": "node node_modules/typescript/bin/tsc",
    "watch": "node node_modules/typescript/bin/tsc --watch"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.45",
    "@types/node": "^6.0.46",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "firefox-profile": "^1.0.1",
    "geckodriver": "^1.7.1",
    "jasmine-reporters": "^2.2.0",
    "protractor": "^5.1.1",
    "protractor-build-verification-testreport": "0.0.4",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "shelljs": "^0.7.8",
    "tslint": "^5.8.0",
    "typescript": "^2.1.0",
    "typescript-eslint-parser": "^0.4.0"
  }
}
